
public class Static {
    int a=10;
    static int b=20;
	public static void main (String ar[]) {
		
		Hello h=new Hello();
		Hello h1=new Hello();
		System.out.println(h.a);
		h.hello();
		h.b=50;
		h1.b=100;
		
		
		System.out.println("h.b: "+h.b);
		
		
	}
	void hello() {
		
	}
	
	static void hey() {
		
	}

}
